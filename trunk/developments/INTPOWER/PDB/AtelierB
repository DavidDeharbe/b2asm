! Uncomment and change the value in the corresponding lines :
!
! xIDx must be replaced by an identifier
! xINTEGERx must be replaced by an integer greater or equal to 1
! xFONTx must be replaced by the name of a character font
! xBOOLx must be replaced by a boolean value (TRUE or FALSE)
! xDIRPATHx must be replaced by the absolute path of a directory
! xPATHx must be replaced by the absolute path of a file
! xIDLISTx  must be replaced by a list of identifiers separated by ","

!================================================================
!   B compiler resources
!================================================================

! Number of character for a tabulation
!ATB*BCOMP*Tab_Width: 8
! Allows parallel substitution in implementations
!ATB*BCOMP*Allow_Parallel: xBOOLx
! Allows PRE substitution in implementations
!ATB*BCOMP*Allow_PRE: xBOOLx
! Allows CHOICE substitution in implementations
!ATB*BCOMP*Allow_CHOICE: xBOOLx
! Allows SELECT substitution in implementations
!ATB*BCOMP*Allow_SELECT: xBOOLx
! Allows ANY substitution in implementations
!ATB*BCOMP*Allow_ANY: xBOOLx
! Allows LET substitution in implementations
!ATB*BCOMP*Allow_LET: xBOOLx
! Allows "becomes member of" substitution in implementations
!ATB*BCOMP*Allow_Becomes_Member_Of: xBOOLx
! Allows "becomes such that" substitution in implementations
!ATB*BCOMP*Allow_Becomes_Such_That: xBOOLx

! Disable B0 controls
! ---------------------------------------------------
!ATB*BCOMP*Disable_Predicate_Syntax_Check: xBOOLx
!ATB*BCOMP*Disable_Expression_Syntax_Check: xBOOLx
!ATB*BCOMP*Disable_Formal_Params_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Concrete_Constants_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Concrete_Variables_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Operation_Input_Parameters_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Operation_Output_Parameters_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Local_Variables_Type_Check: xBOOLx
!ATB*BCOMP*Disable_Variables_Initialisation_Check: xBOOLx
!ATB*BCOMP*Disable_Array_Compatibility_Check: xBOOLx
!ATB*BCOMP*Disable_Parameter_Instanciation_Check: xBOOLx
!ATB*BCOMP*Disable_Valuation_Check: xBOOLx


!================================================================
!   Cross Referencer resources
!================================================================

! General Graph properties
! ------------------------
! Graph Background color
!ATB*GRAPH*BackgroundColor: white	
! Graph Font
!ATB*GRAPH*Font: xFONTx
! Graph default orientation
!ATB*GRAPH*daVinci*Orientation: top_down
! Graph nodes color
!ATB*GRAPH*NodeColor: white
! Components nodes color
!ATB*GRAPH*ComponentNodeColor: white
! Identifiers nodes color
!ATB*GRAPH*IdentNodeColor: white

! Control of graph contents
! -------------------------
! Minimum depth
!ATB*GRAPH*CutBelowDepth: xINTEGERx
! Maximum depth
!ATB*GRAPH*CutAfterDepth: xINTEGERx
! Adds this identifier in the graph
!ATB*GRAPH*AddIdentifier: xIDx
! Adds this components data in the graph
!ATB*GRAPH*AddComponentData: xIDx
! Excludes this identifier from graph
!ATB*GRAPH*ExcludeIdentifier: xIDx
! Excludes this components data from graph
!ATB*GRAPH*ExcludeComponentData: xIDx
! Excludes this library project components data from graph
!ATB*GRAPH*ExcludeLibraryData: xIDx
! Excludes all library components data from graph
!ATB*GRAPH*ExcludeAllLibrariesData: xIDx

! Homonymy Graph
! --------------
! Shape for a concrete variable node
!ATB*GRAPH*HOMO*daVinci*ConcreteVariableShape: box
! Shape for an abstract variable node
!ATB*GRAPH*HOMO*daVinci*AbstractVariableShape: box
! Shape for a concrete variable node
!ATB*GRAPH*HOMO*daVinci*ConcreteConstantShape: circle
! Shape for an abstract constant variable node
!ATB*GRAPH*HOMO*daVinci*AbstractConstantShape: ellipse
! Shape for an abstract set node
!ATB*GRAPH*HOMO*daVinci*AbstractSetShape: triangle

! Formula Graph
! -------------
! Shape for a leaf node
!ATB*GRAPH*FORMULA*daVinci*LeafShape: box
! Shape for an operator node
!ATB*GRAPH*FORMULA*daVinci*OperatorShape: circle
! Shape for a folded formula node
!ATB*GRAPH*FORMULA*daVinci*FoldedShape: ellipse
! Shape for the root node of the graph
!ATB*GRAPH*FORMULA*daVinci*RootShape: rhombus


! Operation Call Graph
! --------------------
! Writes machine names in nodes
!ATB*GRAPH*CALL*PrintMachineName: xBOOLx
! Shape for an operation name
!ATB*GRAPH*CALL*daVinci*OpShape: box


